{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "bbgFiles"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"SFTP_BBG_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SFTP_BBG'"
		},
		"SFTP_BBG_privateKeyContent": {
			"type": "secureString",
			"metadata": "Secure string for 'privateKeyContent' of 'SFTP_BBG'"
		},
		"SFTP_BBG_passPhrase": {
			"type": "secureString",
			"metadata": "Secure string for 'passPhrase' of 'SFTP_BBG'"
		},
		"SFTP_BBG_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "sftp.bloomberg.com"
		},
		"SFTP_BBG_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "emn21629263"
		},
		"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/9e7e6842-f162-4a04-a00e-7f0425277e7a/resourcegroups/Asset_Management/providers/Microsoft.DataFactory/factories/adf-demo-hcdemo-01/integrationruntimes/adf-demo-dfs-shir-02"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bbgFiles_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"SFTP_BBG_password": {
						"value": "[parameters('SFTP_BBG_password')]"
					},
					"SFTP_BBG_privateKeyContent": {
						"value": "[parameters('SFTP_BBG_privateKeyContent')]"
					},
					"SFTP_BBG_passPhrase": {
						"value": "[parameters('SFTP_BBG_passPhrase')]"
					},
					"integrationRuntime1_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('integrationRuntime1_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"SFTP_BBG_properties_typeProperties_host": {
						"value": "[parameters('SFTP_BBG_properties_typeProperties_host')]"
					},
					"SFTP_BBG_properties_typeProperties_userName": {
						"value": "[parameters('SFTP_BBG_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}